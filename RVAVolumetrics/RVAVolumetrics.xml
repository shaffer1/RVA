<ServerManagerConfiguration>
    <ProxyGroup name="sources">
        <SourceProxy name="RVAVolumetrics" class="RVAVolumetrics" label="RVA Volumetrics">
            <Documentation
                long_help = "A wrapped pipeline filter for volumetrics with RVA data sets."
                short_help = "RVA volumetrics.">
            </Documentation>
            <InputProperty
                name="Input"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkDataSet"/>
                </DataTypeDomain>
            </InputProperty>
        </SourceProxy>
    </ProxyGroup>
</ServerManagerConfiguration>
