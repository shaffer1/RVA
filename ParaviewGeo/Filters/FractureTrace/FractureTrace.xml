<ServerManagerConfiguration>
  <!-- Begin OBBtoGrid -->
  <ProxyGroup name="filters">
    <SourceProxy name="Fracture Trace" class="vtkFractureTrace">

      <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPolyData"/>
        </DataTypeDomain>
      </InputProperty>
      
    </SourceProxy>
  </ProxyGroup>
  <!-- End OBBtoGrid -->
</ServerManagerConfiguration>